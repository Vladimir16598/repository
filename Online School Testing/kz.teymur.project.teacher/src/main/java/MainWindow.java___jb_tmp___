        jPanel.add(jLabel);
        jPanel.setBorder(BorderFactory.createEtchedBorder());

        https://image.maps.cit.api.here.com/mia/1.6/mapview
        ?app_id={YOUR_APP_ID}
                &app_code={YOUR_APP_CODE}
    }

    public static JPanel getJPanel(){
